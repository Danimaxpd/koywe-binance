datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "darwin-arm64"]
}

model Trades {
  id          Int      @id @default(autoincrement())
  payment_id  String   @unique
  external_id String
  timestamp   String
  rawData     Json
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
